{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "module": "es6",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "inlineSources": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "target": "es6",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "importHelpers": true
  }
}
