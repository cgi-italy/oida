{
  "name": "@oidajs/eo-video-react",
  "version": "2.0.4",
  "description": "oida EO video adapters react components",
  "module": "es/index.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "less": "styles/index.less",
  "files": [
    "es",
    "lib",
    "styles",
    "types"
  ],
  "scripts": {
    "build:es": "tsc",
    "build:umd": "tsc --project tsconfig.umd.json",
    "clean": "rimraf ./lib ./es ./types ./tsconfig.tsbuildinfo",
    "watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --watch --preserveWatchOutput",
    "lint": "tslint --project ./tsconfig.json",
    "test": "jest --config ./jest.config.js"
  },
  "author": "stefano marra",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "antd": "4.x",
    "classnames": "2.x",
    "lodash": "^4.x",
    "mobx": "6.x",
    "mobx-react": "7.x",
    "moment": "2.x",
    "react": "^16.18.0 || ^17.0.0",
    "react-dom": "^16.18.0 || ^17.0.0"
  },
  "dependencies": {
    "@oidajs/core": "file:../core",
    "@oidajs/eo-mobx": "file:../eo-mobx",
    "@oidajs/eo-mobx-react": "file:../eo-mobx-react",
    "@oidajs/eo-video": "file:../eo-video",
    "@oidajs/state-mobx": "file:../state-mobx",
    "@oidajs/ui-react-antd": "file:../ui-react-antd",
    "@oidajs/ui-react-core": "file:../ui-react-core",
    "@oidajs/ui-react-mobx": "file:../ui-react-mobx",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2"
  }
}
