{
  "name": "@oida/map-cesium",
  "version": "1.2.0",
  "description": "Cesium map renderer wrapper library",
  "module": "es/index.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "config",
    "es",
    "lib",
    "types"
  ],
  "scripts": {
    "build:es": "tsc",
    "build:umd": "tsc --project tsconfig.umd.json",
    "clean": "rimraf ./lib ./es ./types ./tsconfig.tsbuildinfo",
    "watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --watch --preserveWatchOutput",
    "lint": "tslint --project ./tsconfig.json",
    "test": "jest --config ./jest.config.js"
  },
  "author": "stefano marra",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://nexus.dev.eoss-cloud.it/repository/npm-cgi-eo/"
  },
  "dependencies": {
    "@oida/core": "file:../core"
  },
  "peerDependencies": {
    "cesium": "^1.68.0",
    "proj4": "^2.6.0"
  }
}
