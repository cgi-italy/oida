{
  "name": "@oidajs/ui-react-mobx",
  "version": "4.4.2",
  "description": "react hooks and components for state-mobx",
  "module": "es/index.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "types"
  ],
  "scripts": {
    "build:es": "tsc",
    "build:umd": "tsc --project tsconfig.umd.json",
    "clean": "rimraf ./lib ./es ./types ./tsconfig.tsbuildinfo",
    "watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --watch --preserveWatchOutput",
    "lint:check": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx}\" --quiet --fix",
    "test": "jest --config ./jest.config.js"
  },
  "author": "stefano marra",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "classnames": "2.x",
    "lodash": "4.x",
    "mobx": "6.x",
    "mobx-react": "^7.x",
    "react": "^16.18.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.18.0 || ^17.0.0 || ^18.0.0",
    "react-router": "6.x",
    "react-router-dom": "6.x"
  },
  "dependencies": {
    "@oidajs/core": "^4.6.0",
    "@oidajs/state-mobx": "^2.1.2",
    "@oidajs/ui-react-core": "^6.4.2",
    "chroma-js": "^2.4.2",
    "downloadjs": "^1.4.7",
    "lz-string": "^1.4.4",
    "query-string": "^8.1.0",
    "react-cool-dimensions": "^3.0.0",
    "tslib": "^2.4.1"
  }
}
