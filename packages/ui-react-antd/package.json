{
  "name": "@oida/ui-react-antd",
  "version": "2.0.0",
  "description": "antd components",
  "module": "es/index.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "less": "styles/index.less",
  "files": [
    "es",
    "lib",
    "styles",
    "types"
  ],
  "scripts": {
    "pack-icons": "rimraf ./src/icons && svgr --config-file ./config/svgr.config.js --ext tsx --filename-case kebab --out-dir ./src/icons ./assets/icons",
    "prebuild": "npm run pack-icons",
    "build:es": "tsc",
    "build:umd": "tsc --project tsconfig.umd.json",
    "clean": "rimraf ./lib ./es ./types ./tsconfig.tsbuildinfo",
    "watch": "tsc --watch",
    "lint": "tslint --project ./tsconfig.json",
    "test": "jest --config ./jest.config.js"
  },
  "author": "stefano marra",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://nexus.dev.eoss-cloud.it/repository/npm-cgi-eo/"
  },
  "peerDependencies": {
    "antd": "^4.2.4",
    "classnames": "^2.2.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@oida/core": "file:../core",
    "@oida/ui-react-core": "file:../ui-react-core",
    "lodash": "4.17.15",
    "moment": "^2.25.3"
  }
}
