{
  "name": "@oida/ui-react-antd",
  "version": "1.0.0",
  "description": "antd components",
  "typescript:main": "src/index.ts",
  "module": "dist/ui-react-antd.js",
  "main": "dist/ui-react-antd.cjs.js",
  "saas": "dist/styles/index.scss",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src",
    "config"
  ],
  "scripts": {
    "pack-icons": "rimraf ./src/icons && svgr --config-file ./config/svgr.config.js --ext tsx --filename-case kebab --out-dir ./src/icons ./assets/icons",
    "prebuild": "npm run pack-icons",
    "build": "rollup --config rollup.config.js && rollup --config rollup.config.js --environment FORMAT:cjs",
    "clean": "rimraf ./dist ./.rpt2_cache",
    "watch": "rollup --config rollup.config.js --watch",
    "lint": "tslint --project ./tsconfig.json",
    "test": "jest --config jest.config.js"
  },
  "author": "stefano marra",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://nexus.dev.eoss-cloud.it/repository/npm-cgi-eo/"
  },
  "peerDependencies": {
    "antd": "^3.16.2",
    "classnames": "^2.2.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@oida/core": "file:../core",
    "@oida/ui-react-core": "file:../ui-react-core",
    "moment": "^2.22.2"
  }
}
