{
  "name": "@oidajs/eo-adapters-adam",
  "version": "3.2.0",
  "description": "oida EO adam adapters",
  "module": "es/index.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "types"
  ],
  "scripts": {
    "build:es": "tsc",
    "build:umd": "tsc --project tsconfig.umd.json",
