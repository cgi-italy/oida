  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@oidajs/core": "^4.6.0",
    "@oidajs/eo-geotiff": "^3.0.4",
    "@oidajs/eo-mobx": "^3.0.2",
    "@oidajs/map-cesium": "^3.3.2",
    "@oidajs/map-ol": "^3.3.2",
    "@turf/rewind": "^6.5.0",
    "tslib": "^2.4.1"
