{
  "name": "@oidajs/eo-video",
  "version": "2.1.2",
  "description": "oida EO video library",
  "module": "es/index.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "types"
  ],
  "scripts": {
    "build:es": "tsc",
    "build:umd": "tsc --project tsconfig.umd.json",
    "clean": "rimraf ./lib ./es ./types ./tsconfig.tsbuildinfo",
    "watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --watch --preserveWatchOutput",
    "lint:check": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx}\" --quiet --fix",
    "test": "jest --config ./jest.config.js"
  },
  "author": "stefano marra",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "mobx": "6.x"
  },
  "dependencies": {
    "@oidajs/core": "^4.6.0",
    "@oidajs/eo-mobx": "^3.0.2",
    "@oidajs/state-mobx": "^2.1.2",
    "@oidajs/ui-react-mobx": "^4.4.2",
    "mediaelement": "^4.2.16",
    "tslib": "^2.4.1",
    "uuid": "^8.3.2"
  }
}
