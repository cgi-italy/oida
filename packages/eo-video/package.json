{
  "name": "@oida/eo-video",
  "version": "0.1.0",
  "description": "oida EO video library",
  "module": "es/index.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "types"
  ],
  "scripts": {
    "build:es": "tsc",
    "build:umd": "tsc --project tsconfig.umd.json",
    "clean": "rimraf ./lib ./es ./types ./tsconfig.tsbuildinfo",
    "watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --watch --preserveWatchOutput",
    "lint": "tslint --project ./tsconfig.json",
    "test": "jest --config ./jest.config.js"
  },
  "author": "stefano marra",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://nexus.dev.eoss-cloud.it/repository/npm-cgi-eo/"
  },
  "peerDependencies": {
    "mobx": "6.x"
  },
  "dependencies": {
    "@oida/core": "file:../core",
    "@oida/state-mobx": "file:../state-mobx",
    "mediaelement": "^4.2.16",
    "tslib": "^2.0.1",
    "uuid": "^8.0.0"
  }
}
