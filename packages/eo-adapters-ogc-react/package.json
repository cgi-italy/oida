  "author": "stefano marra",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "antd": "4.x",
    "classnames": "2.x",
    "lodash": "^4.x",
    "mobx": "6.x",
    "mobx-react": "7.x",
    "moment": "2.x",
    "react": "^16.18.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.18.0 || ^17.0.0 || ^18.0.0",
    "react-router": "6.x",
    "react-router-dom": "6.x"
  },
  "dependencies": {
    "@oidajs/core": "^4.7.0",
    "@oidajs/eo-adapters-ogc": "^2.4.6",
    "@oidajs/eo-mobx": "^3.2.0",
    "@oidajs/eo-mobx-react": "^4.3.0",
    "@oidajs/state-mobx": "^2.1.3",
    "@oidajs/ui-react-antd": "^6.4.3",
    "@oidajs/ui-react-core": "^6.4.3",
    "@oidajs/ui-react-mobx": "^4.4.3",
    "tslib": "^2.4.1"
  }
}
