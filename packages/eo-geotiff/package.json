{
  "name": "@oidajs/eo-geotiff",
  "version": "3.0.3",
  "description": "oida EO geotiff library",
  "module": "es/index.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "types"
  ],
  "scripts": {
    "build:es": "tsc",
    "build:umd": "tsc --project tsconfig.umd.json",
    "clean": "rimraf ./lib ./es ./types ./tsconfig.tsbuildinfo",
    "watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --watch --preserveWatchOutput",
    "lint:check": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx}\" --quiet --fix",
    "test": "jest --config ./jest.config.js"
  },
  "author": "stefano marra",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "mobx": "6.x",
    "moment": "^2.x",
    "proj4": "^2.x"
  },
  "dependencies": {
    "@oidajs/core": "^4.5.1",
    "@oidajs/eo-mobx": "^3.0.1",
    "@oidajs/state-mobx": "^2.1.1",
    "geotiff": "^2.0.7",
    "lru-cache": "^7.14.1",
    "plotty": "^0.4.9",
    "tslib": "^2.4.1",
    "uuid": "^8.3.2"
  }
}
