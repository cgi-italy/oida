{
  "name": "@oida/eo-ui",
  "version": "1.0.0",
  "description": "oida EO analyst UI library",
  "module": "dist/eo-ui.js",
  "main": "dist/eo-ui.cjs.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
