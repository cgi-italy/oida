    "@oida/eo": "file:../eo",
    "@oida/state-mst": "file:../state-mst",
    "@oida/ui-react-antd": "file:../ui-react-antd",
    "@oida/ui-react-core": "file:../ui-react-core",
    "@oida/ui-react-mst": "file:../ui-react-mst",
    "echarts": "^4.3.0",
    "lodash": "^4.17.15",
